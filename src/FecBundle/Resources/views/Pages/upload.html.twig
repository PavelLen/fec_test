{% extends "@Fec/layout.html.twig" %}
{% block body %}
    <div class="container">
        <div class="row">
            {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
            {{ form_widget(form) }}
            <input type="date" name="from">
            <input type="date" name="to">
            <button type="submit" name="submit">Upload</button>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}